    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "swagger-ui-express": "^5.0.0",
    "express-validator": "^7.0.1",
    "axios": "^1.6.2",
    "date-fns": "^3.0.6",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
